    pathName == "blogs" ||
    pathName == "upcoming_camps" ||
    pathName == "past_camps"
  )
    document.getElementById("articles").className += " menu-item-active";
  else {
    document.getElementById(pathName).className += " menu-item-active";
  }
</script>
