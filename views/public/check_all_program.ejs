                    <p><%-weightPlans[i].content%></p>
                    <a href="/diet" target="_blank">Read More</a>
                  </div>
                </div>
              </div>
              <%if(weightPlans[i+1]){%>
              <div class="item col-md-6">
                <img
                  src="../<%=weightPlans[i+1].image%>"
                  class="img-responsive"
                />

                <div class="slider-box lift">
                  <div class="slider-text">
                    <h4 class=""><%=weightPlans[i+1].name%></h4>
                    <p><%-weightPlans[i+1].content%></p>
                    <a href="/diet" target="_blank">Read More</a>
                  </div>
                </div>
              </div>
              <%}%>
            </div>
          </div>
        </section>
        <%}%>

